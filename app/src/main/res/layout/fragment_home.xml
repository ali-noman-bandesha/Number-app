<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/drawer_layout"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.fragments.HomeFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <include
            android:id="@+id/included_gradient"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            layout="@layout/curve_layout"
            />

        <ImageView
            android:id="@+id/homeIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="60dp"
            app:layout_constraintStart_toStartOf="parent"
            android:src="@drawable/menu_icon"

            />

        <TextView
            android:id="@+id/home_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/Home"
            app:layout_constraintStart_toEndOf="@id/homeIcon"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="50dp"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/quicksand_bold"
            android:textSize="24dp"
            android:textColor="#ffffff"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="30dp"
            />
        <TextView
            android:id="@+id/homeDescription"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/HomeDescription"
            app:layout_constraintStart_toEndOf="@id/homeIcon"
            app:layout_constraintTop_toBottomOf="@id/home_text"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="20dp"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/quicksand_regular"
            android:textSize="12sp"
            android:textColor="#ffffff"
            />

        <WebView
            android:id="@+id/webVeiw"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/included_gradient"
            android:visibility="gone"
            />
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/included_gradient">


            <LinearLayout
                android:id="@+id/homeList"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/included_gradient">

                <RelativeLayout
                    android:id="@+id/numberCard"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:background="@drawable/home_screen_items_bg"
                    android:layout_margin="7dp"
                    android:elevation="3dp"
                    >
                    <ImageView
                        android:id="@+id/numberLocatorIcon"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="20dp"
                        android:elevation="30dp"
                        android:src="@drawable/number_locator_app"

                        />

                    <TextView
                        android:id="@+id/NumberLocatorHeading"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="30dp"
                        android:textSize="16sp"
                        android:layout_toEndOf="@id/numberLocatorIcon"
                        android:fontFamily="@font/quicksand_bold"
                        android:text="@string/NumberLocator"
                        />

                    <TextView
                        android:id="@+id/NumberLocatorDesc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_below="@id/NumberLocatorHeading"
                        android:layout_toEndOf="@id/numberLocatorIcon"
                        android:fontFamily="@font/quicksand_regular"
                        android:text="@string/NumberLocatorDetails"
                        android:layout_centerVertical="true"
                        android:layout_toStartOf="@+id/numberLocatorArrow"
                        android:textSize="12sp"
                        />
                    <ImageView
                        android:id="@+id/numberLocatorArrow"
                        android:layout_width="30dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="30dp"
                        android:src="@drawable/arrow_right"
                        android:layout_marginStart="5dp"

                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/simInfoCard"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:background="@drawable/home_screen_items_bg"
                    android:layout_margin="7dp"
                    android:elevation="3dp"
                    >
                    <ImageView
                        android:id="@+id/blockNumberIcon"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="20dp"
                        android:elevation="10dp"
                        android:src="@drawable/sim_icon" />

                    <TextView
                        android:id="@+id/blockNumberHeading"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="30dp"
                        android:fontFamily="@font/quicksand_bold"
                        android:layout_toEndOf="@id/blockNumberIcon"
                        android:text="@string/simInfo"
                        android:textSize="16sp"
                        />

                    <TextView
                        android:id="@+id/blockDesc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/quicksand_regular"
                        android:layout_below="@id/blockNumberHeading"
                        android:layout_toEndOf="@id/blockNumberIcon"
                        android:text="@string/simDescription"
                        android:layout_centerVertical="true"
                        android:textSize="12sp"
                        android:layout_toStartOf="@+id/blockArrow"
                        />
                    <ImageView
                        android:id="@+id/blockArrow"
                        android:layout_width="30dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="30dp"
                        android:src="@drawable/arrow_right"
                        android:layout_marginStart="5dp"

                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/findNetworkCard"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:background="@drawable/home_screen_items_bg"
                    android:layout_margin="7dp"
                    android:elevation="3dp"
                    >
                    <ImageView
                        android:id="@+id/findNetworkIcon"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="20dp"
                        android:elevation="10dp"
                        android:src="@drawable/find_network_icon" />

                    <TextView
                        android:id="@+id/findNetworkHeading"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="30dp"
                        android:fontFamily="@font/quicksand_bold"
                        android:layout_toEndOf="@id/findNetworkIcon"
                        android:text="@string/findNetwork"
                        android:textSize="16sp"
                        />

                    <TextView
                        android:id="@+id/findNetworkDesc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/quicksand_regular"
                        android:layout_below="@id/findNetworkHeading"
                        android:layout_toEndOf="@id/findNetworkIcon"
                        android:text="@string/networkDescription"
                        android:layout_centerVertical="true"
                        android:textSize="12sp"
                        android:layout_toStartOf="@+id/findNetworkArrow"
                        />
                    <ImageView
                        android:id="@+id/findNetworkArrow"
                        android:layout_width="30dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="30dp"
                        android:src="@drawable/arrow_right"
                        android:layout_marginStart="5dp"

                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/searchCoardCard"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:background="@drawable/home_screen_items_bg"
                    android:layout_margin="7dp"
                    android:elevation="3dp"
                    >
                    <ImageView
                        android:id="@+id/SearchCoordinatesIcon"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="20dp"
                        android:elevation="10dp"
                        android:src="@drawable/search_coordinates_icon" />

                    <TextView
                        android:id="@+id/SearchCoordinatesHeading"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="30dp"
                        android:fontFamily="@font/quicksand_bold"
                        android:layout_toEndOf="@id/SearchCoordinatesIcon"
                        android:text="@string/searchCoordinates"
                        android:textSize="16sp"
                        />

                    <TextView
                        android:id="@+id/SearchCoordinatesDesc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/quicksand_regular"
                        android:layout_below="@id/SearchCoordinatesHeading"
                        android:layout_toEndOf="@id/SearchCoordinatesIcon"
                        android:text="@string/searchCoordinatesDescription"
                        android:layout_centerVertical="true"
                        android:textSize="12sp"
                        android:layout_toStartOf="@+id/SearchCoordinatesArrow"
                        />
                    <ImageView
                        android:id="@+id/SearchCoordinatesArrow"
                        android:layout_width="30dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="30dp"
                        android:src="@drawable/arrow_right"
                        android:layout_marginStart="5dp"

                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/findTrafficCard"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:background="@drawable/home_screen_items_bg"
                    android:layout_margin="7dp"
                    android:elevation="3dp"
                    >
                    <ImageView
                        android:id="@+id/findTrafficIcon"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="20dp"
                        android:elevation="10dp"
                        android:src="@drawable/find_traffic_icon" />

                    <TextView
                        android:id="@+id/findTrafficHeading"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="30dp"
                        android:fontFamily="@font/quicksand_bold"
                        android:layout_toEndOf="@id/findTrafficIcon"
                        android:text="@string/findTraffic"
                        android:textSize="16sp"
                        />

                    <TextView
                        android:id="@+id/findTrafficDesc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/quicksand_regular"
                        android:layout_below="@id/findTrafficHeading"
                        android:layout_toEndOf="@id/findTrafficIcon"
                        android:text="@string/findTrafficDescription"
                        android:layout_centerVertical="true"
                        android:textSize="12sp"
                        android:layout_toStartOf="@+id/findTrafficArrow"
                        />
                    <ImageView
                        android:id="@+id/findTrafficArrow"
                        android:layout_width="30dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="30dp"
                        android:src="@drawable/arrow_right"
                        android:layout_marginStart="5dp"

                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/nearByPlacesCard"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:background="@drawable/home_screen_items_bg"
                    android:layout_margin="7dp"
                    android:elevation="3dp"
                    >
                    <ImageView
                        android:id="@+id/nearByPlacesIcon"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="20dp"
                        android:elevation="10dp"
                        android:src="@drawable/near_by_place_icon" />

                    <TextView
                        android:id="@+id/nearByPlacesHeading"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="30dp"
                        android:fontFamily="@font/quicksand_bold"
                        android:layout_toEndOf="@id/nearByPlacesIcon"
                        android:text="@string/nearByPlaces"
                        android:textSize="16sp"
                        />

                    <TextView
                        android:id="@+id/nearByPlacesDesc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/quicksand_regular"
                        android:layout_below="@id/nearByPlacesHeading"
                        android:layout_toEndOf="@id/nearByPlacesIcon"
                        android:text="@string/placesDescription"
                        android:layout_centerVertical="true"
                        android:textSize="12sp"
                        android:layout_toStartOf="@+id/nearByPlacesArrow"
                        />
                    <ImageView
                        android:id="@+id/nearByPlacesArrow"
                        android:layout_width="30dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="30dp"
                        android:src="@drawable/arrow_right"
                        android:layout_marginStart="5dp"

                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/worldProfilesCard"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:background="@drawable/home_screen_items_bg"
                    android:layout_margin="7dp"
                    android:elevation="3dp"
                    >
                    <ImageView
                        android:id="@+id/worldProfilesIcon"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="20dp"
                        android:elevation="10dp"
                        android:src="@drawable/world_profile_icon" />

                    <TextView
                        android:id="@+id/worldProfilesHeading"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="30dp"
                        android:fontFamily="@font/quicksand_bold"
                        android:layout_toEndOf="@id/worldProfilesIcon"
                        android:text="@string/worldProfiles"
                        android:textSize="16sp"
                        />

                    <TextView
                        android:id="@+id/worldProfilesDesc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/quicksand_regular"
                        android:layout_below="@id/worldProfilesHeading"
                        android:layout_toEndOf="@id/worldProfilesIcon"
                        android:text="@string/worldProfilesDescription"
                        android:layout_centerVertical="true"
                        android:textSize="12sp"
                        android:layout_toStartOf="@+id/worldProfilesArrow"
                        />
                    <ImageView
                        android:id="@+id/worldProfilesArrow"
                        android:layout_width="30dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="30dp"
                        android:src="@drawable/arrow_right"
                        android:layout_marginStart="5dp"

                        />
                </RelativeLayout>

            </LinearLayout>
        </ScrollView>
        <View
            android:background="@drawable/flag_transparent"
            android:id="@+id/view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:visibility="gone"
            />
        <LinearLayout
            android:id="@+id/dialogView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/home_screen_items_bg"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:elevation="5dp"
            android:visibility="gone"
            android:gravity="center_horizontal"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Select Language"
                android:textSize="16sp"
                android:fontFamily="@font/quicksand_bold"
                android:textStyle="bold"
                android:textColor="#777777"
                android:lineSpacingExtra="18sp"
                android:layout_marginTop="20dp"

                />
            <RelativeLayout
                android:id="@+id/english"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@drawable/find_network_et_bg"
                android:layout_marginStart="30dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="30dp"

                >

                <TextView
                    android:id="@+id/englishText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="English"
                    android:textSize="12sp"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="30dp"
                    android:fontFamily="@font/quicksand_bold"
                    android:textStyle="bold"
                    android:textColor="#777777"
                    android:lineSpacingExtra="18sp"
                    />
                <TextView
                    android:id="@+id/englishTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="20dp"
                    android:text="English"
                    android:fontFamily="@font/quicksand_bold"
                    android:textStyle="bold"
                    android:textColor="#777777"
                    android:lineSpacingExtra="18sp"

                    />

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/hindi"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@drawable/find_network_et_bg"
                android:layout_marginStart="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="30dp"

                >

                <TextView
                    android:id="@+id/hindiTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="हिंदी"
                    android:textSize="12sp"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="30dp"
                    android:fontFamily="@font/quicksand_bold"
                    android:textStyle="bold"
                    android:textColor="#777777"
                    android:lineSpacingExtra="18sp"
                    />
                <TextView
                    android:id="@+id/hindiTv1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="20dp"
                    android:text="Hindi"
                    android:fontFamily="@font/quicksand_bold"
                    android:textStyle="bold"
                    android:textColor="#777777"
                    android:lineSpacingExtra="18sp"

                    />

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/german"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@drawable/find_network_et_bg"
                android:layout_marginStart="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="30dp"

                >

                <TextView
                    android:id="@+id/germanTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Deutsch"
                    android:textSize="12sp"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="30dp"
                    android:fontFamily="@font/quicksand_bold"
                    android:textStyle="bold"
                    android:textColor="#777777"
                    android:lineSpacingExtra="18sp"
                    />
                <TextView
                    android:id="@+id/germanTv1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="20dp"
                    android:text="German"
                    android:fontFamily="@font/quicksand_bold"
                    android:textStyle="bold"
                    android:textColor="#777777"
                    android:lineSpacingExtra="18sp"

                    />

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/french"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@drawable/find_network_et_bg"
                android:layout_marginStart="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="30dp"

                >

                <TextView
                    android:id="@+id/frenchTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Français"
                    android:textSize="12sp"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="30dp"
                    android:fontFamily="@font/quicksand_bold"
                    android:textStyle="bold"
                    android:textColor="#777777"
                    android:lineSpacingExtra="18sp"
                    />
                <TextView
                    android:id="@+id/frenchTv1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="20dp"
                    android:text="French"
                    android:fontFamily="@font/quicksand_bold"
                    android:textStyle="bold"
                    android:textColor="#777777"
                    android:lineSpacingExtra="18sp"

                    />

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/azerbaijani"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@drawable/find_network_et_bg"
                android:layout_marginStart="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="30dp"

                >

                <TextView
                    android:id="@+id/azerbijaniTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Azərbaycan"
                    android:textSize="12sp"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="30dp"
                    android:fontFamily="@font/quicksand_bold"
                    android:textStyle="bold"
                    android:textColor="#777777"
                    android:lineSpacingExtra="18sp"
                    />
                <TextView
                    android:id="@+id/azerbijaniTv1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="20dp"
                    android:text="Azerbaijani"
                    android:fontFamily="@font/quicksand_bold"
                    android:textStyle="bold"
                    android:textColor="#777777"
                    android:lineSpacingExtra="18sp"

                    />

            </RelativeLayout>

            <View
                android:layout_marginTop="20dp"
                android:layout_width="wrap_content"
                android:layout_height="0dp"/>
        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
        android:layout_width="250dp"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:layout_gravity="start"
        >

        <include
            android:id="@+id/navDrawer"
            layout="@layout/navigation_drawer_item_design"
            />
    </LinearLayout>

</androidx.drawerlayout.widget.DrawerLayout>